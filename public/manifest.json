{
  "manifest_version": 2,
  "name": "User dashboard",
  "author": "Pooja",
  "version": "1.0.1",
  "description": "Replace new tab page with a personal dashboard.",
  "chrome_url_overrides": {
  "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "geolocation"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
  
 
}
